<!-- Hotspots -->

    <animation>
        <type>pick</type>
        <object-name>Ebrake</object-name>
        <action>
            <button>0</button>
            <repeatable>true</repeatable>
            <interval-sec>0.2</interval-sec>
            <binding>
            <command>property-toggle</command>
            <property>controls/gear/brake-parking</property>
            </binding>
        </action>
    </animation>

<!-- Throttles -->

    <animation>
	<type>rotate</type>
	<object-name>thr1.L</object-name>
	<object-name>thr1.R</object-name>
	<condition>
	    <not>
		<property>controls/engines/engine/reverser</property>
	    </not>
	</condition>
	<property>controls/engines/engine/throttle</property>
	<factor>-40</factor>
	<axis>
	    <x1-m>-9.64</x1-m>
	    <y1-m>-0.19</y1-m>
	    <z1-m>0.66</z1-m>
	    <x2-m>-9.64</x2-m>
	    <y2-m>0.19</y2-m>
	    <z2-m>0.66</z2-m>
	</axis>
    </animation>
    <animation>
	<type>rotate</type>
	<object-name>thr1.L</object-name>
	<object-name>thr1.R</object-name>
	<condition>
		<property>controls/engines/engine/reverser</property>
	</condition>
	<property>controls/engines/engine/throttle</property>
	<factor>15</factor>
	<axis>
	    <x1-m>-9.64</x1-m>
	    <y1-m>-0.19</y1-m>
	    <z1-m>0.66</z1-m>
	    <x2-m>-9.64</x2-m>
	    <y2-m>0.19</y2-m>
	    <z2-m>0.66</z2-m>
