<!--   GEAR -->

        <animation>
        <type>select</type>
        <object-name>unsafe.on</object-name>
        <condition>
            <and>
                <less-than>
                    <property>gear/gear[0]/position-norm</property>
                    <value>1.0</value>
                </less-than>
                <greater-than>
                    <property>gear/gear[0]/position-norm</property>
                    <value>0.0</value>
                </greater-than>
            </and>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>left.on</object-name>
        <condition>
            <equals>
                <property>gear/gear[1]/position-norm</property>
                <value>1.0</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>nose.on</object-name>
        <condition>
            <equals>
                <property>gear/gear[0]/position-norm</property>
                <value>1.0</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>right.on</object-name>
        <condition>
            <equals>
                <property>gear/gear[2]/position-norm</property>
                <value>1.0</value>
            </equals>
        </condition>
    </animation>

